import java.util.Scanner;
/*
 * To change this license header, choose License Headers in Project Properties.
 * To change this template file, choose Tools | Templates
 * and open the template in the editor.
 */

/**
 *
 * @author Aspire E 14
 */
public class MelengkapiScript {
    public static void main(String[] args) {
        
    // Buat Scanner
       Scanner inp = new Scanner (System.in);
       
    // Ambil Nama
       System.out.print("Masukkan Nama Anda = ");
       String Nama = inp.nextLine();
       
    // Ambil NIM
       System.out.print("Masukkan NIM = ");
       String nim = inp.nextLine();
       
    // Ambil Nilai
       System.out.print("Masukkan Nilai = ");
        int nilai = inp.nextInt();
       
       if(nilai > 76) {
            System.out.println("Selamat Anda Lulus!");
        }
        else{
            System.out.println("Maaf Anda Gagal");
       }
    }
}


NO 2

import java.util.Scanner;
/*

 * To change this license header, choose License Headers in Project Properties.
 * To change this template file, choose Tools | Templates
 * and open the template in the editor.
 */

/**
 *
 * @author Aspire E 14
 */
public class NilaiHuruf {
    public static void main(String[] args){
        
    // Buat Scanner
       int nilai;
       String grade = null;
       String nama;
       Scanner scan = new Scanner(System.in);
       
    // Ambil Input
       System.out.print("Nama : ");
       nama = scan.nextLine();
       System.out.print("Nim : ");
       String nim = scan.nextLine();
       System.out.print("Inputkan nilai : ");
       nilai = scan.nextInt();
       
    // Hitung Grade
       if ((nilai >= 86) && (nilai <=100)) {
          System.out.println("A");
       }else if ((nilai >= 76) && (nilai <=85)){
          System.out.println("B");
       }else if ((nilai >= 66) && (nilai <=75)){
          System.out.println("C");
       }else if ((nilai >= 56) && (nilai <=65)){
          System.out.println("D");
       }else if ((nilai >= 0) && (nilai <=55)){
          System.out.println("E");
       }else{
          System.out.println("Error");
       }
    }
    
}


NO 3

import java.util.Scanner;
/*
 * To change this license header, choose License Headers in Project Properties.
 * To change this template file, choose Tools | Templates
 * and open the template in the editor.
 */

/**
 *
 * @author Aspire E 14
 */
public class TinggiBadan {
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        
        System.out.print("Nama : ");
        String nama = input.nextLine();
        
        System.out.print("Jenis Kelamin L/P : ");
        String jenis_kelamin = input.nextLine();
        
        System.out.println("Tinggi Badan : ");
        int tinggi_badan = input.nextInt();
        
        if (jenis_kelamin.equalsIgnoreCase("L")) {
            if (tinggi_badan > 170) {
                System.out.println("Selamat Anda Lulus");
            } else {
                System.out.println("Anda Gagal");
            }
        } else if (jenis_kelamin.equalsIgnoreCase ("P")){
            if (tinggi_badan > 160){
                System.out.println("Selamat Anda Lulus");
            } else {
                System.out.println("Anda Gagal");
            }
        }
    }
}
      
        
NO 4

import java.util.Scanner;
/*
 * To change this license header, choose License Headers in Project Properties.
 * To change this template file, choose Tools | Templates
 * and open the template in the editor.
 */

/**
 *
 * @author Aspire E 14
 */
public class BobotAngka {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        
        String huruf = "";
        System.out.println("Masukkan huruf (A-E)");
        huruf = input.nextLine();
        switch(huruf){
            case "A" :
                System.out.println("4");
                break;
            case "B" :
                System.out.println("3"); 
                break;
            case "C" :
                System.out.println("2");
                break;
            case "D" :
                System.out.println("1");
                break;
            case "E" :
                System.out.println("0");
                break;
                    
        }
    }
}

